{
    "name": "Fidola",
    "description": "Fast, high fidelity algorithms in JavaScript",
    "homepage": "",
    "version": "0.0.1-dev",
    "engines": {
        "node": ">=0.10.0"
    },
    "devDependencies": {
        "mocha": ">=1.6.0",
        "browserify": "~1.16.6",
        "istanbul": ">=0.1.11",
        "uglify-js": "1.3.3",
        "expect.js": ">=0.1.2"
    },
    "license": "BSD-2-Clause",
    "scripts": {
        "test": "mocha -R spec test/test.js && npm run-script coverage",
        "coverage": "npm run-script analyze-coverage && npm run-script check-coverage",
        "analyze-coverage": "node node_modules/istanbul/lib/cli.js cover test/cover.js",
        "check-coverage": "node node_modules/istanbul/lib/cli.js check-coverage --statement -38 --branch -16 --function 97"
    }
}
